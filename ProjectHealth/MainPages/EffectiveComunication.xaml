                    Foreground="DarkSlateGray"
                    FontSize="15"
                    FontStyle="Oblique"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
                    VerticalContentAlignment="Bottom"
                    Content="Effective solutions" Canvas.Left="460" Canvas.Top="-8">
