            <!-- Region (Fourth part) -->
            <Button
                Click="OpenHealthySelfishness"
                    BorderBrush="Transparent"
                    Foreground="DarkSlateGray"
                    FontSize="15"
                    FontStyle="Oblique"
                    FontWeight="SemiBold"
                    HorizontalContentAlignment="Center"
